{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.notifications.hero_new_release",
    "description": "AMQP API for notifications.hero_new_release",
    "properties": {
        "Account-DB": {
            "type": "string"
        },
        "Account-ID": {
            "type": "string"
        },
        "Asset-URL": {
            "description": "URL to download release asset",
            "type": "string"
        },
        "Attachment-URL": {
            "type": "string"
        },
        "Bcc": {
            "type": "string"
        },
        "Cc": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "notification"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "hero_new_release"
            ],
            "type": "string"
        },
        "From": {
            "type": "string"
        },
        "HTML": {
            "type": "string"
        },
        "Hero-Version": {
            "description": "Semver-compliant version number of new release",
            "type": "string"
        },
        "Preview": {
            "type": "string"
        },
        "Reply-To": {
            "type": "string"
        },
        "Subject": {
            "type": "string"
        },
        "Text": {
            "type": "string"
        },
        "To": {
            "type": "string"
        }
    },
    "required": [
        "Account-ID",
        "Asset-URL",
        "Hero-Version"
    ],
    "type": "object"
}
