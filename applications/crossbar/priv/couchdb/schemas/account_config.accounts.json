{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "account_config.accounts",
    "description": "Schema for accounts account_config",
    "properties": {
        "account_types_list": {
            "description": "list of account types",
            "type": "array"
        },
        "default_seat_type": {
            "description": "The default seat type",
            "type": "string"
        },
        "locations": {
            "description": "All locations",
            "properties": {
                "default_location": {
                    "description": "Data for the default user location",
                    "properties": {
                        "display_name": {
                            "description": "The name of the location to display",
                            "type": "string"
                        },
                        "id": {
                            "description": "The ID of this location",
                            "type": "string"
                        },
                        "street_address": {
                            "description": "The actual street address associated with this location",
                            "type": "string"
                        }
                    },
                    "required": [
                        "display_name",
                        "id",
                        "street_address"
                    ],
                    "type": "object"
                },
                "location_list": {
                    "default": [],
                    "description": "list of location objects",
                    "type": "array"
                }
            },
            "required": [
                "default_location"
            ],
            "type": "object"
        },
        "seat_types_list": {
            "description": "list of seat types",
            "type": "array"
        }
    },
    "type": "object"
}
